if table.find(Aids, player.UserId) then
	_G.FPS = fps
	local clock = tick()

	_G.fps = true 
	game:service"RunService".RenderStepped:connect(function()
		if _G.fps == true then
			while clock + 1 / _G.FPS > tick() do end
			wait()
			clock = tick()
		end
	end)
	setfpscap(fps)
end
