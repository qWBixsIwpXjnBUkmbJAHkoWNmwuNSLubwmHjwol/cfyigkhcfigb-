LPlayer.Chatted:Connect(function(msg)
	msg = msg:lower()
	if string.sub(msg,1,3) == "/e " then
		msg = string.sub(msg,4)
	end
	if string.sub(msg,1,1) == prefix then
		local cmd
		local space = string.find(msg," ")
		if space then
			cmd = string.sub(msg,2,space-1)
		else
			cmd = string.sub(msg,2)
		end


		----MAIN------------------------------------------------------------------------------------------------------------------
		-------------------------------------------------------------------------------------------------------------------------------
		-------------------------------------------------------------------------------------------------------------------------------
		-------------------------------------------------------------------------------------------------------------------------------
		if player.Name == host then
			if cmd == "load-host" then
				loadstring(game:HttpGet(loadstringhost))() 
			end
		end

		if cmd == "load-alts"  then	
			if table.find(Aids, player.UserId) then
				loadstring(game:HttpGet(loadstringhost))() 
			end
			if notifications == true then
				if table.find(Aids, player.UserId) then
					game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Loadstring Done!", 'All');
				end
			end
		end


		if cmd == "dropalts" then
			if table.find(Aids, player.UserId) then
				Drops = true
				while Drops == true do
					wait(0.1)
					local DropAm = 10000
					if player.DataFolder.Currency.Value > 10000 then
						DropAm = '10000'
					else
						DropAm = tostring(player.DataFolder.Currency.Value)
					end
					local args = {
						'DropMoney',
						DropAm
					}
					MainEvent:FireServer(unpack(args))
				end
			end
			if notifications == true then
				if table.find(Aids, player.UserId) then
					game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Starting to drop Alts!", 'All');
				end
			end
		end
		if cmd == "setup-me"  then
			if table.find(Aids, player.UserId) then
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = host
				local pos = p1.CFrame
				player.Character.HumanoidRootPart.Anchored = true
			end
		end
		
		if cmd == "zombie" then 
			if table.find(Aids, player.UserId) then
			local Animate = game.Players.LocalPlayer.Character.Animate
			Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=616158929"
			Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=616160636"
			Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=616168032"
			Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
			Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=616161997"
			Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=616156119"
			Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=616157476"
			game.Players.LocalPlayer.Character.Humanoid.Jump = true
			end
			end
		if cmd == "toy" then
			if table.find(Aids, player.UserId) then
			local Animate = game.Players.LocalPlayer.Character.Animate
			Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=782841498"
			Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=782845736"
			Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=782843345"
			Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=782842708"
			Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=782847020"
			Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=782843869"
			Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=782846423"
			game.Players.LocalPlayer.Character.Humanoid.Jump = true
			end
			end
		if cmd == "cartoony" then
			if table.find(Aids, player.UserId) then
			local Animate = game.Players.LocalPlayer.Character.Animate
			Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=782841498"
			Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=782845736"
			Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=782843345"
			Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=782842708"
			Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=782847020"
			Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=782843869"
			Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=782846423"
